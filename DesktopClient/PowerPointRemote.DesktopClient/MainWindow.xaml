<Window x:Class="PowerPointRemote.DesktopClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PowerPointRemote.DesktopClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300">
    <StackPanel>
        <TextBox Text="{Binding ChannelUri, Mode=OneWay}" IsReadOnly="True" />
        <ListBox ItemsSource="{Binding ConnectedUsers}" DisplayMemberPath="Name" />
    </StackPanel>
</Window>